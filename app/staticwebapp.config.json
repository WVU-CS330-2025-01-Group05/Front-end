{
    "routes": [
    {
      "route": "/pages/*",
      "allowedRoles": ["authenticated"]
    },
    {
    "navigationFallback": {
    "rewrite": "/index.html",
    "statusCode": 200
    }
        ],
  "responseOverrides": {
    "401": {
      "statusCode": 302,
      "redirect": "/pages/Login/Login.js"
    }
  }
}
